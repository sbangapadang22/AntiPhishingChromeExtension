{
  "manifest_version": 3,
  "name": "Anti-Phishing Extension",
  "version": "1.0",
  "permissions": ["tabs", "webRequest"],
  "action": {
    "default_popup": "hello.html",
    "default_icon": {
      "16": "hello_extensions.png",
      "48": "hello_extensions.png",
      "128": "hello_extensions.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
